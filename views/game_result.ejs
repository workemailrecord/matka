<script>
    // Show Add modal
    function addModal(gid) {
        $.ajaxModal('#addModal','game_result_add.php?gid='+gid);
    }

    // Create user
    function createUser() {

        // Send ajax Request to ajax/add_user.php  to create a user
        $.easyAjax({
            url: "ajax/game_result_add.php",
            type: "POST",
            container: "#userCreate",
            file:true,
            success: function(response) {
                if (response.status == "success") {
                    $('#addModal').modal('hide');
                     $("#mytable").load(window.location + " #mytable");
                     

                }
            }
        });
    }
    // Show Edit modal
    function editModal(id) {
        $.ajaxModal('#editModal','game_result_edit.php?id='+id);
    }
    // Update user
    function updateUser() {
        $.easyAjax({
            type: "POST",
            url: "ajax/game_result_edit.php",
            container: "#userUpdate",
            file:true,
            success: function(response) {
                if (response.status == "success") {
                    $('#editModal').modal('hide');
                    $("#mytable").load(window.location + " #mytable");
                }
            }
        });
    }
    // Delete user
    function del(id, name) {

        $('#deleteModal').find(".modal-body").html('Are you sure ! You want to delete ?');
        $('#deleteModal').appendTo("body").modal('show');
        $("#delete").click(function () {

            $.easyAjax({
                type: "POST",
                url: "ajax/game_result_delete.php?id="+id,
                container: "#deleteModal",
                success: function (response) {
                    if (response.status == "success") {
                        $('#deleteModal').modal('hide');
                         $("#mytable").load(window.location + " #mytable");
                    }
                }
            });
        })

    }
</script>
