<script src="../assets/global/plugins/bootstrap-fileinput/bootstrap-fileinput.js" type="text/javascript"></script>
<!--<script src="../assets/global/plugins/bootstrap-toastr/toastr.min.js" type="text/javascript"></script>-->
<script src="../assets/plugins/froiden-helper/helper.js" type="text/javascript"></script>
<script>
  // Show Add modal
  function addModal() {
      $.ajaxModal('#addModal', 'add_game.php');
  }

  // Create user
  function createUser() {
      $.easyAjax({
          url: "ajax/add_game.php",
          type: "POST",
          container: "#userCreate",
          file: true,
          success: function(response) {
              console.log(response);
              if (response.status == "success") {
                  $('#addModal').modal('hide');
                  $("#mytable").load(window.location + " #mytable");
              }
          }
      });
  }

  // Show Edit modal
  function editModal(id) {
      $.ajaxModal('#editModal', 'edit_game.php?id=' + id);
  }

  // Update user
  function updateUser() {
      $.easyAjax({
          type: "POST",
          url: "ajax/edit_game.php",
          container: "#userUpdate",
          file: true,
          success: function(response) {
              if (response.status == "success") {
                  $('#editModal').modal('hide');
                  $("#mytable").load(window.location + " #mytable");
              }
          }
      });
  }

  // Delete user
  function del(id) {
      $('#deleteModal').find(".modal-body").html('Are you sure! You want to delete?');
      $('#deleteModal').appendTo("body").modal('show');

      // Use event delegation to bind the click event
      $(document).off('click', '#delete').on('click', '#delete', function() {
          $.easyAjax({
              type: "POST",
              url: "ajax/delete_game.php?id=" + id,
              container: "#deleteModal",
              success: function(response) {
                  if (response.status == "success") {
                      $('#deleteModal').modal('hide');
                      $("#mytable").load(window.location + " #mytable");
                  }
              }
          });
      });
  }
</script>
</body>
</html>